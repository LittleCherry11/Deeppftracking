#========= RCNN ============


layer {
  name: "roi_pool3"
  type: "ROIAlign"
  bottom: "res2c"
  bottom: "rois"
  top: "feature3"
  roi_align_param {
    pooled_w: 7
    pooled_h: 7
    spatial_scale: 0.25 # 1/4
    sampling_ratio: 4
  }
}
